# Template for Kubernetes Secret: ca-saas-db-secret
# Fill in your actual password, then base64 encode it:
# echo -n 'your_password_here' | base64

apiVersion: v1
kind: Secret
metadata:
  name: ca-saas-db-secret
  labels:
    app: ca-saas-db
type: Opaque
data:
  POSTGRES_PASSWORD: <BASE64_ENCODED_PASSWORD>
